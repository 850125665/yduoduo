<resources>

    <declare-styleable name="BaseColor">
        <!--tab 自定义颜色-->
        <attr name="color_source_link" format="color" />
        <attr name="color_tab_text_default" format="color" />
        <attr name="color_tab_indicator" format="color" />
        <!--自定义 loadingView 颜色-->
        <attr name="color_progress" format="color" />
        <attr name="color_progress_background" format="color" />
        <attr name="color_bg_white" format="color" />
        <attr name="color_bg_gray" format="color" />
        <!--基本色 START-->
        <attr name="com_wm_bg" format="color" />
        <attr name="com_space_line" format="color" />
        <attr name="com_main_A" format="color" />
        <attr name="com_main_B" format="color" />
        <attr name="com_assist_A" format="color" />
        <attr name="com_assist_B" format="color" />
        <attr name="com_font_A" format="color" />
        <attr name="com_font_B" format="color" />
        <attr name="com_font_C" format="color" />
        <attr name="com_font_D" format="color" />
        <attr name="com_dialog_bg" format="color" />
        <!--基本色 END-->
    </declare-styleable>


    <attr name="item_count" format="integer" />
    <attr name="text_color_normal" format="color" />
    <attr name="text_color_hightlight" format="color" />
    <attr name="text_size" format="dimension" />
    <attr name="indicator_color" format="color" />
    <attr name="indicator_src" format="reference" />
    <attr name="indicator_style">
        <enum name="bitmap" value="0" />
        <enum name="line" value="1" />
        <enum name="square" value="2" />
        <enum name="triangle" value="3" />
    </attr>

    <!-- RVPIndicator -->
    <declare-styleable name="RVPIndicator">
        <attr name="item_count" />
        <attr name="text_color_normal" />
        <attr name="text_color_hightlight" />
        <attr name="text_size" />
        <attr name="indicator_color" />
        <attr name="indicator_src" />
        <attr name="indicator_style" />
    </declare-styleable>

    <declare-styleable name="TagGroup">
        <attr name="atg_isAppendMode" format="boolean" />
        <attr name="atg_inputHint" format="string" />

        <attr name="atg_borderColor" format="color" />
        <attr name="atg_textColor" format="color" />
        <attr name="atg_backgroundColor" format="color" />

        <attr name="atg_dashBorderColor" format="color" />
        <attr name="atg_inputHintColor" format="color" />
        <attr name="atg_inputTextColor" format="color" />

        <attr name="atg_checkedBorderColor" format="color" />
        <attr name="atg_checkedTextColor" format="color" />
        <attr name="atg_checkedMarkerColor" format="color" />
        <attr name="atg_checkedBackgroundColor" format="color" />
        <attr name="atg_pressedBackgroundColor" format="color" />
        <attr name="atg_borderStrokeWidth" format="dimension" />
        <attr name="atg_textSize" format="dimension" />
        <attr name="atg_horizontalSpacing" format="dimension" />
        <attr name="atg_verticalSpacing" format="dimension" />
        <attr name="atg_horizontalPadding" format="dimension" />
        <attr name="atg_verticalPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr name="tagGroupStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="superrecyclerview">
        <attr name="layout_empty" format="reference" />
        <attr name="layout_progress" format="reference" />
        <attr name="layout_error" format="reference" />
        <attr name="recyclerClipToPadding" format="boolean" />
        <attr name="recyclerPadding" format="dimension" />
        <attr name="recyclerPaddingTop" format="dimension" />
        <attr name="recyclerPaddingBottom" format="dimension" />
        <attr name="recyclerPaddingLeft" format="dimension" />
        <attr name="recyclerPaddingRight" format="dimension" />
        <attr name="scrollbarStyle">
            <enum name="insideOverlay" value="0x0" />
            <enum name="insideInset" value="0x01000000" />
            <enum name="outsideOverlay" value="0x02000000" />
            <enum name="outsideInset" value="0x03000000" />
        </attr>
        <attr name="scrollbars">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="0" />
            <enum name="none" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="XlHRatingBar">
        <attr name="starCount" format="integer" /><!--总的星星数-->
        <attr name="canEdit" format="boolean" /><!--是否可以点击选择，有时候纯展示用不需要选择-->
        <attr name="widthAndHeight" format="dimension" /><!--单个星星的宽度-->
        <attr name="dividerWidth" format="dimension" /><!--星星左右间距-->
        <attr name="countSelected" format="integer" /><!--当前已选中的星星个数-->
        <attr name="stateResId" format="reference" /><!--单个星星不同状态下对应的图标-->
        <attr name="differentSize" format="boolean" /><!--是否支持大小不等星星样式-->
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="loadding_color" format="color" />
        <attr name="ring_style" format="enum">
            <enum name="square" value="0" />
            <enum name="round" value="1" />
        </attr>
        <attr name="progress_style" format="enum">
            <enum name="material" value="0" />
            <enum name="linear" value="1" />
        </attr>

        <attr name="ring_width" format="dimension" />
        <attr name="ring_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DirectionalViewpager">
        <attr name="direction" format="string">
        </attr>
    </declare-styleable>

    <!--显示提示的View-->
    <declare-styleable name="TipView">
        <attr name="tipBackgroundColor" format="color"/>
        <attr name="tipTextColor" format="color"/>
        <attr name="tipText" format="string"/>
        <attr name="tipTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PowerfulRecyclerView">
        <attr name="dividerColor" format="color"/>
        <attr name="dividerSize" format="dimension"/>
        <attr name="dividerDrawable" format="reference"/>
        <attr name="useStaggerLayout" format="boolean"/>
        <attr name="numColumns" format="integer"/>
        <attr name="rvOrientation" format="enum">
            <enum name="HORIZONTAL" value="0"/>
            <enum name="VERTICAL" value="1"/>
        </attr>
        <attr name="dividerMarginLeft" format="dimension"/>
        <attr name="dividerMarginRight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="BorderTextView">
        <attr name="strokeWidth" format="dimension"/>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="strokeColor" format="color"/>
        <attr name="followTextColor" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RadarView">
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="android:text"/>
        <attr name="labelTextSize" format="dimension"/>
        <attr name="labelTextColor" format="color"/>
        <attr name="labelText" format="string"/>
        <attr name="format" format="string"/>
        <attr name="sideColor" format="color"/>
        <attr name="outsideBackgroundColor" format="color"/>
        <attr name="insideBackgroundColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="textOffsetY" format="dimension"/>
        <attr name="labelTextOffsetY" format="dimension"/>

        <attr name="circleColor" format="color"/>
        <attr name="lineColor" format="color"/>
        <attr name="showLine" format="boolean"/>
        <attr name="rotate" format="integer"/>
        <attr name="showLabel" format="boolean"/>
        <attr name="showText" format="boolean"/>
        <attr name="scanTime" format="integer"/>
        <attr name="insideStrokeWidth" format="dimension"/>
        <attr name="outsideStrokeWidth" format="dimension"/>
        <attr name="lineStrokeWidth" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="NumberProgressBar">
        <attr name="progress" format="integer" />
        <attr name="max" format="integer" />

        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />

        <attr name="progress_reached_bar_height" format="dimension" />
        <attr name="progress_unreached_bar_height" format="dimension" />

        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />

        <attr name="progress_text_offset" format="dimension" />
    </declare-styleable>

</resources>